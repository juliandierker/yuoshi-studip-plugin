{
    "parser": "@typescript-eslint/parser",
    "extends": [
        "react-app",
        "plugin:prettier/recommended"
    ],
    "plugins": [
        "@typescript-eslint",
        "react"
    ],
    "rules": {
        "prettier/prettier": "warn",
        "import/order": ["warn", {
            "newlines-between": "always"
        }]
    }
}
